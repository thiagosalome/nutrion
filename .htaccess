RewriteEngine On

RewriteRule ^/?$ index.php?Controller=nutricionista&Action=login
RewriteRule ^([a-zA-Z0-9]+)/?$ index.php?Controller=$1&Action=login
RewriteRule ^([a-zA-Z0-9]+)/([a-zA-z0-9]+)$ index.php?Controller=$1&Action=$2
# RewriteRule ^([a-zA-Z0-9]+)/([a-zA-z0-9]+)/([a-zA-z0-9]+)$ index.php?Controller=$1&Action=$2&id=$3
RewriteRule ^([a-zA-Z0-9]+)/([a-zA-z0-9]+)/([a-zA-z0-9]+)$ index.php?Controller=$1&ControllerDashboard=$2&ActionDashboard=$3
RewriteRule ^([a-zA-Z0-9]+)/([a-zA-z0-9]+)/([a-zA-z0-9]+)/([0-9]+)$ index.php?Controller=$1&ControllerDashboard=$2&ActionDashboard=$3&id=$4